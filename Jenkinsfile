pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Scripting') {
            steps {
                parallel:{
                    "TaskOne":{
                        echo "task one job 1"
                        echo "task one job 2"
            },
                    "TaskTwo":{
                        echo "task one job 1"
        }
    }
}
